{
  "name": "Devexsolutions/doctrine-user-repository",
  "description": "Mezzio Authentication Doctrine User Repository",
  "license": "BSD-3-Clause",
  "authors": [{
    "name": "Joseph Koch"
  }],
  "require": {
    "php": "^8.0",
    "mezzio/mezzio-authentication": "^1.1",
    "doctrine/orm": "^2.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "webimpress/coding-standard": "^1.0.6",
    "laminas/laminas-coding-standard": "2.0.0rc1"
  },
  "autoload": {
    "psr-4": {
      "Devexsolutions\\DoctrineUserRepository\\": "src/"
    }
  },
  "extra": {
    "laminas" : {
      "config-provider": "Devexsolutions\\DoctrineUserRepository\\ConfigProvider"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
